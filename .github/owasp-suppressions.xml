<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <suppress>
        <notes><![CDATA[
            The vulnerability is about Files.createTempDir from Guava, that is not being used in this project
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.google\.guava/guava@.*$</packageUrl>
        <vulnerabilityName>CVE-2020-8908</vulnerabilityName>
    </suppress>
    <suppress>
        <notes><![CDATA[
           gdx-jnigen-loader misidentified as a screenshot upload bash script available on GitHub that had an URL predictability vulnerability
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.badlogicgames\.gdx/gdx\-jnigen\-loader@.*$</packageUrl>
        <cpe>cpe:/a:utils_project:utils</cpe>
    </suppress>
</suppressions>
